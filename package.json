  "peerDependencies": {
    "mocha": "3.x"
  },
  "devDependencies": {
    "@types/js-beautify": "0.0.28",
    "@types/mkdirp": "^0.3.29",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.41",
    "@types/socket.io": "^1.4.27",
    "rollup": "^0.36.1",
    "rollup-plugin-typescript": "^0.8.1",
