    "mkdirp": "0.5.1",
    "pretty-data": "0.40.0",
    "socket.io": "1.4.8",
    "st": "1.2.0"
  },
  "bundledDependencies": [
    "diff2html",
    "difflib",
    "jimp",
    "mithril",
    "mkdirp",
    "pretty-data"
